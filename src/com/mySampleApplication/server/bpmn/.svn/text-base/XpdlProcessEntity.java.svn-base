package com.mySampleApplication.server.bpmn;

import java.util.Date;
import java.util.Set;

/**
 * This class represents a XPDL entry.
 */
public class XpdlProcessEntity {

    private Long dbid;
    private String name;
    private String xpdl;

    public XpdlProcessEntity() {
    }


    public XpdlProcessEntity(String name, String xpdl) {
        this.name = name;
        this.xpdl = xpdl;
    }


    public Long getDbid() {
        return dbid;
    }

    public void setDbid(Long dbid) {
        this.dbid = dbid;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getXpdl() {
        return xpdl;
    }

    public void setXpdl(String xpdl) {
        this.xpdl = xpdl;
    }

    @Override
    public String toString() {
        return this.getName();
    }
}
